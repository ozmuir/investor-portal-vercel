{
  "functions": {
    "api/**/*": {
      "includeFiles": "api-lib/**"
    }
  },
  "rewrites": [
    {
      "source": "/investor/:file(.*\\..*)",
      "destination": "https://vercel-investor-portal.vercel.app/:file"
    },
    {
      "source": "/investor/api/:match*",
      "destination": "https://vercel-investor-portal.vercel.app/api/:match*"
    }
  ]
}
